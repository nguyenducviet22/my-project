package test;

import java.util.Random;

public class Test2 {
	public static void main(String[] args) {
		Random rd = new Random();
		int value = rd.nextInt(1, 1000);
		System.out.println(value);
		String randomCode1 = System.currentTimeMillis() + value + "";
		String randomCode2 = System.currentTimeMillis() + value + "";
		String randomCode3 = System.currentTimeMillis() + value + "";
		String randomCode3 = System.currentTimeMillis() + value + "";
		System.out.println(randomCode1);
		System.out.println(randomCode2);
		System.out.println(randomCode3);
	}
}
